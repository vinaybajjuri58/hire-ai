{
  "extends": "next/core-web-vitals",
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_",
        "caughtErrors": "none"
      }
    ],
    "@typescript-eslint/no-explicit-any": ["warn"],
    "react-hooks/exhaustive-deps": ["warn"],
    "react/no-unescaped-entities": [
      "error",
      {
        "forbid": [">", "}"]
      }
    ]
  }
}
